# Solana Configuration
NEXT_PUBLIC_SOLANA_NETWORK=mainnet
NEXT_PUBLIC_SOLANA_RPC_HOST=https://api.mainnet-beta.solana.com

# Database Configuration
DATABASE_URL=postgresql://username:password@db.example.com:5432/meme_coin_platform

# Security
NEXTAUTH_SECRET=your_very_long_and_complex_secret_key
NEXTAUTH_URL=https://meme-coin-platform.com

# Monitoring
DATADOG_API_KEY=your_datadog_api_key
SENTRY_DSN=your_sentry_dsn

# External Services
JUPITER_SWAP_API_KEY=your_jupiter_api_key
PYTH_NETWORK_API_KEY=your_pyth_network_api_key

# Rate Limiting
RATE_LIMIT_REQUESTS=100
RATE_LIMIT_WINDOW_MS=60000

# Feature Flags
ENABLE_AI_TRADING_BOTS=true
ENABLE_TOKEN_LAUNCH=true
